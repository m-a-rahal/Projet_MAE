                                    <Label layoutX="14.0" layoutY="14.0" text="Résultats du test" />
                                    <BarChart fx:id="chart" layoutY="39.0" prefHeight="291.0" prefWidth="636.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="39.0">
