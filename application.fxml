                                        <CategoryAxis label="Algorithmes" side="BOTTOM" />
                                      </xAxis>
                                      <yAxis>
                                        <NumberAxis label="temps d'execution (ms)" side="LEFT" />
                                      </yAxis>
                                    </BarChart>
                                    <Button fx:id="test_btn" layoutX="112.0" layoutY="10.0" mnemonicParsing="false" onAction="#run_test" text="tester" />
                                 </children>
                              </AnchorPane>
                          </items>
                        </SplitPane>
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
         </children>
    </AnchorPane>
  </children>
